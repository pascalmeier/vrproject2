<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vrproject2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="18" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.example.vrproject2.ParseApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.vrproject2.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.vrproject2.LoginFragment"
            android:label="@string/title_activity_login_fragment"
            android:parentActivityName="Fragment" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="Fragment" />
        </activity>
        <activity
            android:name="com.example.vrproject2.SignupFragment"
            android:label="@string/title_activity_signup_fragment"
            android:parentActivityName="Fragment" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="Fragment" />
        </activity>
        <activity
            android:name="com.example.vrproject2.MenuActivity"
            android:label="@string/title_activity_menu" >
        </activity>
    </application>

</manifest>
